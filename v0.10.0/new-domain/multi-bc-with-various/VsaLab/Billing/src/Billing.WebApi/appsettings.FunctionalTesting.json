{
  "AllowedHosts": "*",
  "UseInMemoryBus": true,
  "UseInMemoryDatabase": true,
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ]
  },
  "JwtSettings": {
    "Audience": "",
    "Authority": "",
    "AuthorizationUrl": "",
    "TokenUrl": "",
    "ClientId": ""
  }
}
